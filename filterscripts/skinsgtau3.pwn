public OnPlayerCommandText(playerid, cmdtext[])
{
	if(!strcmp("/skinsgtau", cmdtext, true)) return ShowPlayerDialog(playerid, DialogSkinsGTAU, DIALOG_STYLE_LIST, "{FF0000}# {FFFFFF}Skins GTA: Underground", "GTA III\r\nGTA Vice City\r\nGTA Liberty City (Stories)", "Selecionar", "Cancelar");
	if(!strcmp("/skinsiii", cmdtext, true)) return CallLocalFunction("OnDialogResponse", "dddds", playerid, DialogSkinsGTAU, 1, 0, "null");
	if(!strcmp("/skinsvc", cmdtext, true)) return CallLocalFunction("OnDialogResponse", "dddds", playerid, DialogSkinsGTAU, 1, 1, "null");
	if(!strcmp("/skinslcs", cmdtext, true)) return CallLocalFunction("OnDialogResponse", "dddds", playerid, DialogSkinsGTAU, 1, 2, "null");
	return 0;
}
public OnDialogResponse(playerid, dialogid, response, listitem, inputtext[]) {
	switch(dialogid) {
		case DialogSkinsGTAU: {
			if(!response) return 1;
			new ListSkins[16 * 128];
			switch(listitem) {
			    case 0: {
					for(new i = 21000; i <= 21114; i++)
					    format(ListSkins, sizeof(ListSkins), "%s%i\n", ListSkins, i);
					ShowPlayerDialog(playerid, DialogSkinsGTAUIII, DIALOG_STYLE_PREVMODEL, "Skins GTAU: III", ListSkins, "Selecionar", "Cancelar");
				}
			    case 1: {
					for(new i = 21115; i <= 21293; i++)
					    format(ListSkins, sizeof(ListSkins), "%s%i\n", ListSkins, i);
					ShowPlayerDialog(playerid, DialogSkinsGTAUVC, DIALOG_STYLE_PREVMODEL, "Skins GTAU: Vice City", ListSkins, "Selecionar", "Cancelar");
				}
			    case 2: {
					for(new i = 21294; i <= 21425; i++)
					    format(ListSkins, sizeof(ListSkins), "%s%i\n", ListSkins, i);
					ShowPlayerDialog(playerid, DialogSkinsGTAULCS, DIALOG_STYLE_PREVMODEL, "Skins GTAU: Liberty City Stories", ListSkins, "Selecionar", "Cancelar");
				}

			}
			return 1;
		}
		case DialogSkinsGTAUIII: {
		    if(!response) return CallLocalFunction("OnPlayerCommandText", "ds", playerid, "/skinsgtau");
			return SetPlayerSkin(playerid, listitem + 21000);
		}
		case DialogSkinsGTAUVC: {
		    if(!response) return CallLocalFunction("OnPlayerCommandText", "ds", playerid, "/skinsgtau");
			return SetPlayerSkin(playerid, listitem + 21115);
		}
		case DialogSkinsGTAULCS: {
		    if(!response) return CallLocalFunction("OnPlayerCommandText", "ds", playerid, "/skinsgtau");
			return SetPlayerSkin(playerid, listitem + 21294);
		}
	}
	return 1;
}
